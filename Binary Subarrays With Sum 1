class Solution {
public:
    int subarraySum(vector<int>& nums, int k) {
        int ans = 0;
        for(int i = 0 ; i< nums.size() ; i++){
            int temp = 0;
            temp += nums[i];
            if(temp == k)ans++;
            for(int j = i+1 ; j<nums.size() ; j++){
                temp += nums[j];
                if(temp == k)ans++;
                //else if(temp > k)break;
            }
        }
        return ans;
    }
}
   
