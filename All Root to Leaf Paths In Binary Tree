#include<bits/stdc++.h>
string s = "";
void traverse(BinaryTreeNode < int > * root , vector<string>&ans){
    if (root == NULL) return;
    if(root->left == NULL && root->right == NULL){
        s += to_string(root->data) ;
        ans.push_back(s);
        int i = s.size() - 1;
        while(i>=0 && s[i] != ' '){
            s.pop_back();
            i--;
        }
        return;
    }

    s += to_string(root->data) + " " ;
    
    traverse(root->left , ans);
    traverse(root->right,ans);
    s.pop_back();
    int i = s.size() - 1;
    while(i>=0 && s[i] != ' '){
        s.pop_back();
        i--;
    }
    return;
    
    
}

vector < string > allRootToLeaf(BinaryTreeNode < int > * root) {
    // Write your code here.
    vector<string>ans;
    traverse(root , ans);
    return ans;

}
