#include<bits/stdc++.h>

int priority(char x){
	if(x == '^')return 3;
	else if(x == '*' || x == '/')return 2;
	else if(x == '+' || x == '-')return 1;
	else{
		return -1;
	}
}

string infixToPostfix(string exp){
	// Write your code here
	stack<char>st;
	string ans = "";

	for(int i = 0 ; i < exp.size() ; i++){
		if ((exp[i] >= '0' && exp[i] <= '9') ||
    (exp[i] >= 'a' && exp[i] <= 'z') ||
    (exp[i] >= 'A' && exp[i] <= 'Z')){
		ans += exp[i];
		continue;
	}

		else if(exp[i] == '('){
			st.push(exp[i]);
			continue;
		}
		else if (exp[i] == ')'){
			while(st.top() != '('){
				ans += st.top();
				st.pop();
			}
			st.pop();
			continue;
		}

		while(!st.empty() && priority(st.top()) >= priority(exp[i])){
			ans += st.top();
			st.pop();
		}
		st.push(exp[i]);

		
		
		 
	}
	while(!st.empty()){
		ans += st.top();
		st.pop();
	}
	return ans;
}
